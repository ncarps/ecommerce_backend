{
  "kind": "collectionType",
  "collectionName": "customer",
  "info": {
    "name": "Customer"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "customerId": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "string",
      "required": true
    },
    "mobileNumber": {
      "type": "string",
      "required": true
    },
    "addresses": {
      "collection": "address"
    }
  }
}
